{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "com.haojia.test.student",
		"type": "component",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"embeddedBy": "../"
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {}
			}
		},
		"rootView": {
			"viewName": "com.haojia.test.student.view.StudentApp",
			"type": "XML",
			"async": true
		},
		"routing": {
			"config": {
				"async": true,
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.haojia.test.student.view",
				"transition": "show",
				"controlId": "studentApp",
				"clearTarget": true,
				"controlAggregation": "pages"
			},
			"routes": [{
				"name": "StudentMain",
				"pattern": "",
				"target": "StudentMain"
			}, {
				"name": "StudentList",
				"pattern": "list",
				"target": "StudentList"
			}, {
				"name": "EditStudent",
				"pattern": "edit/{id}",
				"target": "EditStudent"
			}],
			"targets": {
				"StudentMain": {
					"viewName": "StudentMain",
					"viewLevel": 0,
					"transition": "show"
				},
				"StudentList": {
					"viewName": "StudentList",
					"viewLevel": 0,
					"transition": "show"
				},
				"EditStudent": {
					"viewName": "EditStudent",
					"viewLevel": 1,
					"transition": "slide"
				}
			}
		}
	}
}

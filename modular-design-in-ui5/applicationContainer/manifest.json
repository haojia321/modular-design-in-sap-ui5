{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "com.haojia.test.app",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"embeds": ["student", "exam"]
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"componentUsages": {
			"student": {
				"name": "com.haojia.test.student",
				"lazy": true
			},
			"exam": {
				"name": "com.haojia.test.exam",
				"lazy": true
			}
		},
		"dependencies": {
			"minUI5Version": "1.66",
			"libs": {
				"sap.m": {}
			}
		},
		"rootView": {
			"viewName": "com.haojia.test.applicationContainer.view.App",
			"type": "XML",
			"async": true
		},
		"routing": {
			"config": {
				"async": true,
				"routerClass": "sap.m.routing.Router",
				"transition": "show",
				"controlId": "container",
				"controlAggregation": "pages"
			},
			"routes": [{
				"pattern": ["", "student"],
				"name": "student",
				"target": {
					"name": "student",
					"prefix": "student-prefix"
				}
			}, {
				"pattern": "exam",
				"name": "exam",
				"target": {
					"name": "exam",
					"prefix": "exam-prefix"
				}
			}],
			"targets": {
				"student": {
					"type": "Component",
					"usage": "student",
					"id": "student"
				},
				"exam": {
					"type": "Component",
					"usage": "exam",
					"id": "exam"
				}
			}
		}
	}
}
